{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"no-unused-vars": "off",
		"no-mixed-spaces-and-tabs": "off",
		"react/prop-types": "off",
		"react/no-unknown-property": "off"
	}
}
